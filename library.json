{
  "name": "Crypto++",
  "version": "8.9.1",
  "description": "A 1:1 port of the official Crypto++ library on GitHub by weidai11",
  "keywords": "crypto, crypto++, cryptopp, aes, rsa, sha, sha1, sha2, sha3, md5, serpent, twofish, blowfish, simeck, threefish, hmac, blake2b, blake2s, cmac, dsa, elgamal, pkcs, keccack, ecdsa",
  "build": {
    "srcDir": ".",
    "includeDir": ".",
    "srcFilter": "+<**/*.cpp>",
    "unflags": "-fno-rtti",
    "flags": "-frtti -D CRYPTOPP_DISABLE_ASM=1 -D CRYPTOPP_DISABLE_MIXED_ASM=1"
  },
  "examples": [
    {
      "name": "MD5 Hash",
      "base": "examples",
      "files": [
        "mf5.cpp"
      ]
    },
    {
      "name": "AES-128 GCM",
      "base": "examples",
      "files": [
        "aes-gcm.cpp"
      ]
    }
  ],
  "authors": [
    {
      "name": "weidai11",
      "url": "https://github.com/weidai11/cryptopp"
    },
    {
      "name": "TheClockTwister",
      "url": "https://github.com/TheClockTwister/cryptopp-pio",
      "maintainer": true
    }
  ],
  "license": "BSL-1.0",
  "homepage": "https://github.com/TheClockTwister/cryptopp-pio",
  "dependencies": {},
  "frameworks": "*",
  "platforms": "*"
}